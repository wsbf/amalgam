{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "noImplicitAny": false,
        "strictNullChecks": false,
        "sourceMap": true,
        "baseUrl": "../viewSrc",
	"paths": {
            "*": [
                "../node_modules/*",
                "src/types/*"
            ]
        }
    },
    "include": [
	"./src/**/*"
    ]
}
